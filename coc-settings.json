{
    "coc.preferences.useQuickfixForLocations": true,
    "prettier.singleQuote": true,
    "prettier.semi": false,
    "prettier.trailingComma": "all",
    "prettier.tabWidth": 4,
    "prettier.bracketSpacing": true,
    "prettier.printWidth": 100,
    "diagnostic.checkCurrentLine": true,
    "codeLens.enable": true,
    "java.jdt.ls.lombokSupport.enabled": true,
    "java.jdt.ls.java.home": "/usr/lib/jvm/default",
    "java.jdt.ls.vmargs": "-XX:+UseParallelGC -XX:GCTimeRatio=4 -XX:AdaptiveSizePolicyWeight=90 -Dsun.zip.disableMemoryMapping=true -Xmx12G -Xms100m -Xlog:disable",
    "java.maven.downloadSources": true,
    "java.import.maven.enabled": true,
    "java.saveActions.organizeImports": true,
    "java.format.enabled": true,
    "java.referencesCodeLens.enabled": true,
    "java.implementationsCodeLens.enabled": true,
    "java.completion.maxResults": 50,
    "java.inlayHints.parameterNames.enabled": "all",
    "java.sources.organizeImports.starThreshold": 99,
    "sumneko-lua.enableNvimLuaDev": true,
    "languageserver": {
        "csharp-ls": {
            "command": "csharp-ls",
            "filetypes": ["cs"],
            "rootPatterns": ["*.csproj", ".vim/", ".git/", ".hg/"]
        },
        "golang": {
            "command": "gopls",
            "rootPatterns": ["go.mod", ".vim/", ".git/", ".hg/"],
            "filetypes": ["go"],
            "initializationOptions": {
                "usePlaceholders": true,
                "completeUnimported": true
            }
        }
    },
    "suggest.completionItemKindLabels": {
        "keyword": " Keyword",
        "variable": " Variable",
        "value": "󰎠 Value",
        "operator": " Operator",
        "function": " Function",
        "reference": " Reference",
        "constant": " Constant",
        "method": " Method",
        "struct": " Struct",
        "class": " Class",
        "interface": " Interface",
        "text": " Text",
        "enum": " Enum",
        "enumMember": " EnumMember",
        "module": " Module",
        "color": " Color",
        "property": " Property",
        "field": " Field",
        "unit": " Unit",
        "event": " Event",
        "file": " File",
        "folder": " Folder",
        "snippet": " Snippet",
        "typeParameter": " TypeParameter",
        "default": " "
    },
    "suggest.detailField": "preview",
    "suggest.enableFloat": true,
    "suggest.pumFloatConfig": {
        "border": true,
        "highlight": "CocFloating",
        "rounded": true
    },
    "suggest.floatConfig": {
        "border": true,
        "rounded": true,
        "highlight": "CocFloating"
    },
    "hover.floatConfig": {
        "border": true,
        "rounded": true,
        "highlight": "CocFloating"
    },
    "floatFactory.floatConfig": {
        "border": true,
        "rounded": true,
        "highlight": "CocFloating"
    },
    "Lua.workspace.checkThirdParty": false,
    "cSpellExt.enabledDictionaries": ["portuguese-brazilian"],
    "cSpell.language": "en,pt,pt_BR",
    "cSpell.enabled": true,
    "cSpell.ignorePaths": [
        "node_modules",        // this will ignore anything the node_modules directory
        "**/node_modules",     // the same for this one
        "**/node_modules/**",  // the same for this one
        "node_modules/**",     // Doesn't currently work due to how the current working directory is determined.
        "vscode-extension",    //
        ".git",                // Ignore the .git directory
        "*.dll",               // Ignore all .dll files.
        "**/*.dll"             // Ignore all .dll files
